INTERFACE Math;

(* see documentation for python module math *)

CONST pi = 3.1415926535897931;
CONST e = 2.7182818284590451;

PROCEDURE acos(x: REAL): REAL;

PROCEDURE asin(x: REAL): REAL;

PROCEDURE atan(x: REAL): REAL;

PROCEDURE atan2(x: REAL): REAL;

PROCEDURE ceil(x: REAL): REAL;

PROCEDURE cos(x: REAL): REAL;

PROCEDURE cosh(x: REAL): REAL;

PROCEDURE degrees(x: REAL): REAL;

PROCEDURE exp(x: REAL): REAL;

PROCEDURE fabs(x: REAL): REAL;

PROCEDURE floor(x: REAL): REAL;

PROCEDURE fmod(x, y: REAL): REAL;

PROCEDURE fmantisse(x: REAL): REAL;

PROCEDURE fexp2(x: REAL): INTEGER;

PROCEDURE hypot(x: REAL): REAL;

PROCEDURE ldexp(x: REAL): REAL;

PROCEDURE log(x: REAL; base: INTEGER): REAL;

PROCEDURE log10(x: REAL): REAL;

PROCEDURE modf(x: REAL): REAL;

PROCEDURE pow(x,y: REAL): REAL;

PROCEDURE radians(x: REAL): REAL;

PROCEDURE sin(x: REAL): REAL;

PROCEDURE sinh(x: REAL): REAL;

